{
  "name": "hirasso/rh-admin-utils",
  "description": "A WordPress utility plugin ðŸ¥ž",
  "license": "GPL-2.0-or-later",
  "autoload": {
    "psr-4": {
      "RH\\AdminUtils\\": "src"
    }
  },
  "type": "wordpress-plugin",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=8.2",
    "composer/installers": "^1.0 || ^2.0",
    "symfony/var-dumper": "^7.1",
    "yahnis-elsts/plugin-update-checker": "^5.5"
  },
  "scripts": {
      "prefix-namespaces": [
          "./scripts/prefixNamespaces.sh"
      ],
      "post-install-cmd": [
          "@prefix-namespaces"
      ],
      "post-update-cmd": [
          "@prefix-namespaces"
      ]
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.11"
  },
  "config": {
    "allow-plugins": {
      "composer/installers": true
    }
  }
}
